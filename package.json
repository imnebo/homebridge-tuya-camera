{
  "displayName": "Homebridge Tuya Camera",
  "name": "homebridge-tuya-camera",
  "version": "0.0.3",
  "description": "The [Homebridge](https://homebridge.io) Tuya Camera plugin allows you to get the rtsp or hls stream for your Tuya Cameras.",
  "author": "donavanbecker",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/donavanbecker/homebridge-tuya-camera.git"
  },
  "bugs": {
    "url": "https://github.com/donavanbecker/homebridge-tuya-camera/issues"
  },
  "engines": {
    "homebridge": "^1.4.0",
    "node": "^14.18.2 || ^16.13.1"
  },
  "main": "dist/index.js",
  "scripts": {
    "check": "npm install && npm outdated",
    "update": "ncu -u && npm update && npm install",
    "lint": "eslint src/**.ts",
    "watch": "npm run build && npm link && nodemon",
    "build": "rimraf ./dist && tsc",
    "prepublishOnly": "npm run lint && npm run build"
  },
  "funding": [
    {
      "type": "Paypal",
      "url": "https://www.paypal.me/donavanbecker"
    }
  ],
  "keywords": [
    "homebridge-plugin",
    "tuya-camera",
    "tuya camera",
    "tuya",
    "camera",
    "rtsp"
  ],
  "dependencies": {
    "@homebridge/plugin-ui-utils": "^0.0.19",
    "axios": "^0.26.1",
    "crypto-js": "^4.1.1",
    "rxjs": "^7.5.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/node": "^17.0.21",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^8.10.0",
    "homebridge": "^1.4.0",
    "nodemon": "^2.0.15",
    "npm-check-updates": "^12.5.2",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2",
    "typescript-axios-wb": "^1.0.3"
  }
}
